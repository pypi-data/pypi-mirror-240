    {
      "id": "#didcomm",
      "type": "did-communication",
      "priority": 0,
      "recipientKeys": [
        "#1"
      ],
      "routingKeys": [
        "did:key:z6Mknq3MqipEt9hJegs6J9V7tiLa6T5H5rX3fFCXksJKTuv7#z6Mknq3MqipEt9hJegs6J9V7tiLa6T5H5rX3fFCXksJKTuv7"
      ],
      "serviceEndpoint": "http://bob:3000"

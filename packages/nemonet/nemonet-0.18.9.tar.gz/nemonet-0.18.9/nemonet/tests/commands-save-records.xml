<graph>
    <nodes>
        <node name="node1"/>
        <node name="node2"/>
        <node name="node3"/>
        <node name="node4"/>
        <node name="node5"/>
    </nodes>
    <edges>
        <edge from="node1" to="node2"/>
        <edge from="node2" to="node3"/>
        <edge from="node3" to="node4"/>
        <edge from="node4" to="node5"/>
    </edges>
    <actions>
        <action xpath="" type="OPENURL" nodename="node1" value="https://jqueryui.com/droppable/" waitafter="10"/>
        <action xpath="" type="SAVE_CURRENT_URL" nodename="node2" key="cu" waitafter="10"/>
        <action xpath="" type="SPLIT_STORED_URL" nodename="node3" key="cu" waitafter="10"/>
        <action xpath="" type="FORMAT_STRINGS" nodename="node4" key="co_url" value="https://jqueryui.com/#{cu_2}" waitafter="10"/>
        <action xpath="" type="DUMMY" nodename="node5" value=""/>
    </actions>
</graph>
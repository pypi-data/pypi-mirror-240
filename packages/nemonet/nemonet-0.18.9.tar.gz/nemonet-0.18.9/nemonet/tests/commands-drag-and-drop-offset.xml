<graph>
    <nodes>
        <node name="node1"/>
        <node name="node2"/>
        <node name="node3"/>
        <node name="node4"/>
    </nodes>
    <edges>
        <edge from="node1" to="node2"/>
        <edge from="node2" to="node3"/>
        <edge from="node3" to="node4"/>
    </edges>
    <actions>
        <action xpath="" type="OPENURL" nodename="node1" value="https://jqueryui.com/draggable/" waitafter="10"/>
        <action xpath="//*[@id='draggable']/p" type="DRAG_AND_DROP_WITH_OFFSET" nodename="node3" value="100,0"/>
        <action xpath="" type="GOTO_FRAME" nodename="node2" value="0"/>
        <action xpath="" type="DUMMY" nodename="node4" value=""/>
    </actions>
</graph>
<graph>
    <nodes>
        <node name="node1"/>
        <node name="node2"/>
        <node name="node3"/>
    </nodes>
    <edges>
        <edge from="node1" to="node2"/>
        <edge from="node2" to="node3"/>
    </edges>
    <actions>
        <action xpath="" type="OPENURL" nodename="node1" value="https://jqueryui.com/draggable/" waitafter="10"/>
        <action xpath="" type="REMOVE_HTML_ELEMENT" nodename="node2" value="#logo-events" waitafter="10"/>
        <action xpath="" type="DUMMY" nodename="node3" value=""/>
    </actions>
</graph>